pipeline {
    agent any
    triggers {
        pollSCM('*/2 * * * *')
    }
    stages {
        stage('Pull source code') {
            steps {
                echo "Pulling "
//                 git 'https://github.com/maifujalam/openshift.git'
                   git branch: 'master',url: 'https://github.com/maifujalam/openshift.git'
                   echo "debug"
                   sh "git branch"
            }
        }
        stage('Build'){
            steps{
                echo "building"

            }
        }
    }
}
